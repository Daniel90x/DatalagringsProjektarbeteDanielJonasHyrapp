<UserControl x:Class="Store.SubViews.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Store.SubViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="680">
    <!--
        d:DesignHeight="450" d:DesignWidth="680" kan sättas till det måttet
        man förväntar sig när UserControl:en används men det det är inte satt
        i sten. UserControls kan också stretchas som alla andra kontroller.
    -->
    <Grid>
        <!--
            Här är det bara att fylla på med vad som helst! :D 
            Inga gränser och C# kod som bara gäller här kan
            läggas till i 'Home.xaml.cs' med.
        -->
        <Image Source="../Images/Home.png" Stretch="Fill"></Image>
    </Grid>
</UserControl>
