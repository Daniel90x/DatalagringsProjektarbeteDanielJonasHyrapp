<Window x:Class="Store.Lek"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Store"
        mc:Ignorable="d"
        Title="Lek" Height="450" Width="800">
    <Grid Name="Menu" Width="120" HorizontalAlignment="Left" ShowGridLines="True">
        <Grid.RowDefinitions>
            <RowDefinition Height="50" Name="YourMovies"></RowDefinition>
            <RowDefinition Height="50" Name="Genre_Action"></RowDefinition>
            <RowDefinition Height="50" Name="Genre_Adventure"></RowDefinition>
            <RowDefinition Height="50" Name="Genre_Thriller"></RowDefinition>
            <RowDefinition Height="50" Name="Genre_Drama"></RowDefinition>
            <RowDefinition Height="50" Name="Genre_Romance"></RowDefinition>

            <!-- Logout längst ner -->
        </Grid.RowDefinitions>

    </Grid>
</Window>




    